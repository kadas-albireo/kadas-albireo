<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE qgisgrassmodule SYSTEM "http://mrcc.com/qgisgrassmodule.dtd">

<qgisgrassmodule label="Export raster to Geo TIFF" module="r.out.gdal">
	<option key="input" region="no"/>
	<option key="format" answer="GTiff" hidden="yes" />
	<option key="type" label="Choose appropriate format"/>
	<file key="output" type="new" filters="Geo TIFF (*.tif *.tiff)" />
</qgisgrassmodule>
