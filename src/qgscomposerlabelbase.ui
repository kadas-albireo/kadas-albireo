        <signal>clicked()</signal>
        <receiver>QgsComposerLabelBase</receiver>
        <slot>changeFont()</slot>
    </connection>
    <connection>
        <sender>mTextLineEdit</sender>
        <signal>returnPressed()</signal>
        <receiver>QgsComposerLabelBase</receiver>
        <slot>textChanged()</slot>
    </connection>
</connections>
<slots>
    <slot>changeFont()</slot>
    <slot>textChanged()</slot>
</slots>
<layoutdefaults spacing="6" margin="11"/>
</UI>
