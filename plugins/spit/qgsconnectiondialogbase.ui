        <cstring>QgsConnectionDialogBase</cstring>
    </property>
    <property name="geometry">
        <rect>
            <x>0</x>
            <y>0</y>
            <width>411</width>
            <height>230</height>
        </rect>
    </property>
    <property name="caption">
        <string>Connection Properties</string>
    </property>
    <widget class="QGroupBox">
