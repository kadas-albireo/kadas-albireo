                    <font>
                    </font>
                </property>
                <property name="text">
                    <string>Test Connect</string>
                </property>
            </widget>
            <widget class="QCheckBox" row="5" column="1" rowspan="1" colspan="2">
                <property name="name">
                    <cstring>chkStorePassword</cstring>
