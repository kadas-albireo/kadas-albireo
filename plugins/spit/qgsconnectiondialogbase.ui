            <widget class="QLabel" row="2" column="0">
                <property name="name">
                    <cstring>lblDatabase</cstring>
                </property>
                <property name="font">
                    <font>
                        <family>Sans</family>
                    </font>
                </property>
                <property name="text">
                    <string>Database</string>
                </property>
            </widget>
            <widget class="QLineEdit" row="3" column="1">
                <property name="name">
                    <cstring>txtUsername</cstring>
                </property>
            </widget>
            <widget class="QLabel" row="3" column="0">
